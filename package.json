{
  "name": "henri-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config build/webpack.client.js",
    "build:client": "webpack --config build/webpack.client.js --mode production",
    "build:server": "webpack --config build/webpack.server.js --mode production",
    "build": "yarn build:client && yarn build:server",
    "serve": "node src/server",
    "docs:dev": "node genDocsConfig && vuepress dev docs",
    "docs:build": "node genDocsConfig && vuepress build docs"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "koa": "^2.11.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.11",
    "vuepress-plugin-code-copy": "^1.0.4",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-loader": "^4.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
